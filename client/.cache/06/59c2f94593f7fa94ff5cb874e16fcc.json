et filter = obj.filter(ob => {\n          return ob[0] === dat\n        })\n          if (filter.length === 0){\n            obj.push([dat, 1])\n          }else{\n            obj[obj.indexOf(filter[0])][1]++\n          }\n        })\n        console.log(obj)\n        this.tags = obj\n      })\n      .catch(err => {\n        console.log(err.response.data)\n      })\n    }\n  },\n  mounted() {\n    this.getTags()\n  }\n\n}\n</script>\n\n<style>\n\n</style>\n"},"lineCount":null}},"error":null,"hash":"8684fe0ecc909a2ea5272c2828be5778","cacheData":{"env":{}}}