","name":null,"original":{"line":93,"column":0},"generated":{"line":124,"column":34}},{"source":"src/components/createArticle.vue","name":null,"original":{"line":94,"column":0},"generated":{"line":125,"column":0}},{"source":"src/components/createArticle.vue","name":null,"original":{"line":79,"column":0},"generated":{"line":125,"column":7}},{"source":"src/components/createArticle.vue","name":null,"original":{"line":95,"column":0},"generated":{"line":126,"column":0}},{"source":"src/components/createArticle.vue","name":null,"original":{"line":65,"column":0},"generated":{"line":127,"column":0}},{"source":"src/components/createArticle.vue","name":null,"original":{"line":47,"column":0},"generated":{"line":128,"column":0}}],"sources":{"src/components/createArticle.vue":"<template>\n  <div>\n    <form @submit.prevent=\"createArticle\">\n      <div class=\"form-group\">\n        <label for=\"titleForm\" style=\"color:white\">Title</label>\n        <input\n          v-model=\"article.title\"\n          type=\"text\"\n          class=\"form-control\"\n          id=\"titleForm\"\n          placeholder=\"Title\"\n        >\n      </div>\n      <div class=\"form-group\">\n          <h6 style=\"color:white;text-align:center\">Image</h6>\n        <div class=\"container\">\n          <div class=\"container\" style>\n            <picture-input\n              ref=\"pictureInput\"\n              width=\"100\"\n              height=\"100\"\n              :custom-strings=\"{\n        upload: '<h1>Bummer!</h1>',\n        drag: `<img\n            style='margin-right: 60px;height: 100px;width:100px;'\n            src='http://icons.iconarchive.com/icons/dtafalonso/android-lollipop/256/Camera-Next-icon.png'\n            alt='Card image cap'\n          >`\n      }\"\n            ></picture-input>\n            <p v-if=\"error\" style=\"color:red;display:flex;justify-content:center\">{{ error }}</p>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"form-group\">\n        <Vueditor style=\"height:300px;margin-top:30px\" ref=\"wisi\"></Vueditor>\n      </div>\n      <button type=\"submit\" class=\"btn btn-primary\">Create</button>\n    </form>\n  </div>\n</template>\n\n<script>\nconst url = `http://34.87.13.129`;\nimport PictureInput from \"vue-picture-input\";\nexport default {\n  name: \"navbar-login\",\n  props: [],\n  data() {\n    return {\n      article: {\n        title: \"\",\n        content: \"\",\n        image: \"\",\n        tag: \"\"\n      },\n      error: false\n    };\n  },\n  components: {\n    PictureInput\n  },\n  created() {},\n  methods: {\n    createArticle() {\n      this.article.content = this.$refs.wisi.getContent();\n      console.log(this.$refs.wisi)\n      let newImage = new FormData();\n      newImage.append(\"image\", this.$refs.pictureInput.file);\n      newImage.append(\"title\", this.article.title);\n      newImage.append(\"content\", this.article.content);\n      //   newImage.append(\"tags\", this.tags);\n      console.log(newImage);\n      for (var data of newImage) {\n        console.log(data);\n      }\n\n      axios({\n        method: \"POST\",\n        url: `${url}/home/`,\n        headers: {\n          token: localStorage.getItem(\"token\")\n        },\n        data: newImage\n      })\n        .then(({ data }) => {\n          console.log(data);\n          this.$emit(\"showArticle\");\n        })\n        .catch(error => {\n          this.error = error.response.data.message;\n          console.log(error.response);\n        });\n    }\n  }\n};\n</script>"},"lineCount":null}},"error":null,"hash":"97d8d306f4c3bd64deeb799a2a0a1d8a","cacheData":{"env":{}}}